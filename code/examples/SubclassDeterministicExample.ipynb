    "from numpy.random import choice\n",
    "\n",
    "jobs_rates_pay = {'blacksmith':(.2,50),'farmer':(.5,20),'governor':(.01,1000),\n",
    "               'Landlord':(.05,200),'builder':(.2,50),'merchant':(.04,250),'weaver':(0,5),\n",
    "               'adolescent':(0,2),'child':(0,0),'elderly':(0,0)}\n",
    "childhood = 7\n",
