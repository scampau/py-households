    "        self.jobprobs = jobprobs\n",
    "        self.profession = None\n",
    "        self.money = 0\n",
    "        self.father = father\n",
    "        self.mother = mother\n",
    "        self.childhood = childhood\n",
    "        self.adolescence = adolescence\n",
    "        self.oldage = oldage\n",
    "        \n",
    "    \n",
    "    def pro_dev(self):\n",
    "\n",
